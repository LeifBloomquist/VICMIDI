     16  a8ac
     17  a8ac		       8c ba a8 	      sty	ch0	; 4
     18  a8af		       8c c5 a8 	      sty	ch1	; 4
     19  a8b2		       be c9 a8 	      ldx	ldfqmasks-$a,y	; 4
     20  a8b5		       85 57		      sta	TMP	; 3
     21  a8b7
