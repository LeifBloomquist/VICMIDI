     27  a8bc		       84 58		      sty	TMP2	; 3
     28  a8be		       a0 07		      ldy	#7	; 2
     29  a8c0
     30  a8c0				   l0
     31  a8c0		       a9 7f		      lda	#$7f	; 2
     32  a8c2		       07 57		      .byte.b	$07,TMP	; aso TMP		 ; 5  asl tmp; a = [tmp] OR $7f  *ILLEGAL OPCODE*
     33  a8c4		       8f 0c 90 	      .byte.b	$8F,$0C,$90	; axs $900c	 ; 4  [$900c] = a AND x	  *ILLEGAL OPCODE*
