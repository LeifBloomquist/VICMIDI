      0  a8a9					      include	"setwave.asm"
      1  a8a9							;A short generic routine for setting any shift register value for any pulse
      2  a8a9							;channel in about 150 cpu clocks. Use it freely.
      3  a8a9
      4  a8a9							; USAGE: y = channel ($0a..$0c)
      5  a8a9							;	  x = initial frequency
      6  a8a9							;	  a = shift register contents
      7  a8a9							;
      8  a8a9							; WARNING for purists: self-modifying code, illegal opcodes.
      9  a8a9							;
     10  a8a9							; code align assertion: make sure that the loop is within a page.
     11  a8a9							; oscillator assertion: make sure that the channel has been at $7e
     12  a8a9							; for some time before calling this function.
     13  a8a9							; put TMP and TMP2 in the zero page.
     14  a8a9
