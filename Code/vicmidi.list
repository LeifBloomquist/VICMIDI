     45  a587		       a0 08		      ldy	#$08	; For 4MHz crystal
     46  a589		       8e 01 9c 	      stx	UART_DIVISOR_MSB
     47  a58c		       8c 00 9c 	      sty	UART_DIVISOR_LSB
     48  a58f
